{
  "name": "sass",
  "version": "1.0.0",
  "description": "translating-sass",
  "scripts": {
    "compilar-sass": "sass  fuente/sass/pages:fuente/css --source-map",
    "monitorizar-sass": "sass --watch fuente/sass/pages:fuente/css --style expanded --source-map",
    "start": "parcel \"fuente/html/*.html\"",
    "build": "parcel build \"fuente/html/*.html\" --public-url ./"
  },
  "author": "Ignacio Palomar Chacon",
  "license": "ISC",
  "devDependencies": {
    "parcel": "^2.13.3",
    "sass": "^1.85.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.7.2"
  },
  "directories": {
    "doc": "docs"
  }
}
